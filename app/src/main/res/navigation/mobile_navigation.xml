<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/mobile_navigation"
        app:startDestination="@+id/nav_register_can">

    <fragment
            android:id="@+id/nav_register_can"
            android:name="com.durand.dogedex.ui.user_fragment.register_can.RegisterCanFragment"
            tools:layout="@layout/fragment_register_can" >
        <action
            android:id="@+id/action_nav_register_can_to_nav_register_can_can_raza"
            app:destination="@id/nav_register_can_can_raza" />
        <action
            android:id="@+id/action_nav_register_can_to_nav_my_can_register"
            app:destination="@id/nav_my_can_register" />
    </fragment>

    <fragment
        android:id="@+id/nav_register_can_can_raza"
        android:name="com.durand.dogedex.ui.user_fragment.register_can.RegisterCanDangerousFragment"
        tools:layout="@layout/fragment_register_can_dangerous" />

    <fragment
            android:id="@+id/nav_register_hocio"
            android:name="com.durand.dogedex.ui.user_fragment.register_hocico.RegisterHocicoFragment"
            tools:layout="@layout/fragment_register_hocico" />

    <fragment
            android:id="@+id/nav_my_can_lost"
            android:name="com.durand.dogedex.ui.user_fragment.my_can_lost.CanPerdidoFragment"
            tools:layout="@layout/fragment_can_perdido">
        <action
            android:id="@+id/action_nav_my_can_lost_to_nav_can_report_lost"
            app:destination="@id/nav_can_report_lost" />
    </fragment>

    <fragment
            android:id="@+id/nav_my_can_register"
            android:name="com.durand.dogedex.ui.user_fragment.list_my_can_register.MyCanRegisterFragment"
            tools:layout="@layout/fragment_my_can_register">
        <action
            android:id="@+id/action_nav_my_can_register_to_nav_my_can_register_detail"
            app:destination="@id/nav_my_can_register_detail"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
            android:id="@+id/nav_my_can_register_detail"
            android:name="com.durand.dogedex.ui.user_fragment.list_my_can_register.MyCanRegisterDetailFragment"
            tools:layout="@layout/fragment_my_can_register_detail" />

    <fragment
            android:id="@+id/nav_can_report_lost"
            android:name="com.durand.dogedex.ui.user_fragment.list_can_report_lost.CanReportLostFragment"
            tools:layout="@layout/fragment_can_report_lost">
        <action
            android:id="@+id/action_nav_can_report_lost_to_nav_can_report_lost_detail"
            app:destination="@id/nav_can_report_lost_detail"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
            android:id="@+id/nav_can_report_lost_detail"
            android:name="com.durand.dogedex.ui.user_fragment.list_can_report_lost.CanReportLostDetailFragment"
            tools:layout="@layout/fragment_can_report_lost_detail" />

    <fragment
            android:id="@+id/nav_map_canes_perdidos"
            android:name="com.durand.dogedex.ui.user_fragment.mapa_canes_perdidos.MapaCanesPerdidosFragment"
            tools:layout="@layout/fragment_mapa_canes_perdidos">
        <action
            android:id="@+id/action_nav_map_canes_perdidos_to_nav_can_perdido_detail"
            app:destination="@id/nav_can_perdido_detail"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
            android:id="@+id/nav_can_perdido_detail"
            android:name="com.durand.dogedex.ui.user_fragment.mapa_canes_perdidos.CanPerdidoDetailFragment"
            tools:layout="@layout/fragment_can_perdido_detail" />

    <fragment
            android:id="@+id/nav_close"
            android:name="com.durand.dogedex.ui.CloseFragment"
            tools:layout="@layout/fragment_close" />

</navigation>